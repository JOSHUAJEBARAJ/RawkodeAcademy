
dev:
  #!/usr/bin/env sh
  # tag::local-dev[]
  infisical run -- supabase start

  set -o allexport
  eval $(infisical run -- supabase status -o env --override-name  auth.anon_key=SUPABASE_ANON_KEY | tail -n +2)
  set +o allexport

  cd web
  bun install
  infisical run -- bun dev
  # end::local-dev[]
